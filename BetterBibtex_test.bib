@article{araujoInelasticNeutronScattering2017,
  title = {Inelastic Neutron Scattering Study of Reline: Shedding Light on the Hydrogen Bonding Network of Deep Eutectic Solvents},
  shorttitle = {Inelastic Neutron Scattering Study of Reline},
  author = {Araujo, C. F. and Coutinho, J. A. P. and Nolasco, M. M. and Parker, S. F. and {Ribeiro-Claro}, P. J. A. and Rudi{\'c}, S. and Soares, B. I. G. and Vaz, P. D.},
  year = {2017},
